  <thead>
    <tr class="bg-custom-primary">
      <th width="5%">
        <input id="js-check-all" class="js-checkbox-all" type="checkbox">
      </th>
      <th width="20%">
        <%= __('Fullname') %>
      </th>
      <th width="15%">
        <%= __('Email Address') %>
      </th>
      <th width="15%">
        <%= __('Phone') %>
      </th>
      <th width="25%">
        <%= __('Relationships') %>
      </th>
