<div class="row" id="menuDetail" data-classactive="<%= classActive %>" data-coursesession="<%= currCourseSession.id %>">
  <div class="col-12 grid-margin d-flex justify-content-between">
    <p class="card-description"><%= __('Menu') %> <span class="dateMenu"><%= __('Today') %></span></p>

    <div class="row">
      <div class="col-12">
        <div>
          <% if(menuObj) { %>
          <div id="editMenu" data-toggle="modal" data-target="#modalMenu" data-backdrop="static"
          data-keyboard="false" class="btn btn-primary"><%= __('Edit') %></div>
          <% }%>
            <div id="addMenu" data-toggle="modal" data-target="#modalMenu" data-backdrop="static"
          data-keyboard="false"class="btn btn-primary"><%= __('Add new') %></div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-12">
    <div class="accordion" id="accordion" role="tablist">
      <% if(menuObj) {
          for (let i = 0; i < menuObj.length; i++) {
      %>
      <div class="card">
        <div class="card-header" role="tab" id="heading-<%= i %>">
          <h6 class="mb-0">
            <a data-toggle="collapse" href="#collapse-<%= i %>" aria-expanded="<%= i == 0 ? true : false%>"
              aria-controls="collapse-<%= i %>">
              <%= menuObj[i].name %> ( <%= menuObj[i].timeTitleStart %> -> <%= menuObj[i].timeTitleEnd %>)
            </a>
          </h6>
        </div>
        <div id="collapse-<%= i %>" class="collapse <%= i == 0 ? 'show' : ''%>" role="tabpanel"
          aria-labelledby="heading-<%= i %>" data-parent="#accordion">
          <div class="card-body">
            <div class="row">
              <div class="col-12">
                <ul class="bullet-line-list">
                  <%
                    for (let j = 0; j < menuObj[i].list.length; j++){
                  %>
                  <li>
                    <h6 class="font-weight-bold"><%= menuObj[i].list[j].time %></h6>
                    <% 
                    for (let k = 0; k <  menuObj[i].list[j].food.length; k++){
                    %>
                    <p><%= menuObj[i].list[j].food[k].title %></p>
                    <%}%>
                  </li>
                  <%}%>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <%}} else {%>
      <div class="menuEmpty">
        <div class="icon-box-secondary icon-box-lg mb-4">
          <i class="mdi mdi-food-variant"></i>
        </div>
        <p><%= __('No data') %></p>
      </div>
      <%}%>
    </div>
  </div>
</div>