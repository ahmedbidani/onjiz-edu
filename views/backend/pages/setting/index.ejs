                              <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#social" role="tab"> <span class="hidden-xs-down"><%= __('Social networking')%></span></a> </li>
                              <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#widget" role="tab"><span class="hidden-xs-down"><%= __('Widget')%></span></a> </li>
                          </ul>
                          <!-- Tab panes -->
                          <div class="tab-content">
                              <div class="tab-pane active" id="generalWeb" role="tabpanel">
                                  <div class="p-20">
                                    <div class="form-group">
                                        <label><%= __('Name')%></label>
                                        <input type="text" class="form-control" value="<%= web && web.value && web.value.name ? web.value.name : '' %>" id="webName" name="webName">
                                        <span class="text-help"></span>
                                    </div>
                                    <div class="form-group">
                                        <label><%= __('Description')%></label>
                                      <input type="text" class="form-control" value="<%= web && web.value && web.value.description ? web.value.description : '' %>" id="webDescription" name="webDescription">
                                      <span class="text-help"></span>
                                    </div>
                                    <div class="form-group">
                                      <label><%= __('Version')%></label>
                                      <input type="text" class="form-control" value="<%= web && web.value && web.value.version ? web.value.version : ''%>" id="webVersion" name="webVersion">
                                      <span class="text-help"></span>
                                    </div> 
                                    <div class="form-group">
                                        <label><%= __('License')%></label>
                                        <input type="text" class="form-control" value="<%= web && web.value && web.value.license ? web.value.license : '' %>" id="webLicense" name="webLicense">
                                        <span class="text-help"></span>
                                    </div> 
                                    <div class="form-group">
                                      <label><%= __('Language')%></label>
                                      <select id="webLanguage" name="webLanguage" class="form-control js-process-basic-multiple w-100"
                                      data-placeholder="<%= __('Choose') %>">
                                      <option value="">---</option>
                                        <% for (let language of languages) { %>
                                        <option value="<%= language.code %>" <%=web&&web.value&&web.value.language&&web.value.language==language.code?'selected':''%>>
                                          <%= language.name %>
                                        </option>
                                        <% } %>
                                      </select>
                                    </div>
                                    <div class="form-group">
                                      <label><%= __('Display name')%></label>
                                      <select id="webDisplayName" name="webDisplayName" class="form-control js-process-basic-multiple w-100"
                                      data-placeholder="<%= __('Choose') %>">
                                        <option value="firstlast" <%=web&&web.value&&web.value.displayName&&web.value.displayName=='firstlast'?'selected':''%>><%= __('First/Last Name')%></option>
                                        <option value="lastfirst" <%=web&&web.value&&web.value.displayName&&web.value.displayName=='lastfirst'?'selected':''%>><%= __('Last/First Name')%></option>
                                        <!-- <option value="first" <%=web&&web.value&&web.value.displayName&&web.value.displayName=='first'?'selected':''%>><%= __('First Name')%></option>
                                        <option value="last" <%=web&&web.value&&web.value.displayName&&web.value.displayName=='last'?'selected':''%>><%= __('Last Name')%></option> -->
                                      </select> 
                                    </div>
                                    <div class="form-group">
                                      <label><%= __('Date format')%></label>
                                      <fieldset>
                                        <label><input type="radio" name="webDateFormat" value="MMM DD YYYY" <%= (web && web.value && web.value.dateFormat == "MMM DD YYYY")? 'checked="checked"': ''%>> <span class="date-time-text format-i18n">July 05 2019</span> [<code>MM D Y</code>]</label><br>
                                        <label><input type="radio" name="webDateFormat" value="YYYY/MM/DD" <%= (web && web.value && web.value.dateFormat == "YYYY/MM/DD")? 'checked="checked"': ''%>> <span class="date-time-text format-i18n">2019-07-15</span> [<code>Y-m-d</code>]</label><br>
                                        <label><input type="radio" name="webDateFormat" value="MM/DD/YYYY" <%= (web && web.value && web.value.dateFormat == "MM/DD/YYYY")? 'checked="checked"': ''%>> <span class="date-time-text format-i18n">07/15/2019</span> [<code>m/d/Y</code>]</label><br>
                                        <label><input type="radio" name="webDateFormat" value="DD/MM/YYYY" <%= (web && web.value && web.value.dateFormat == "DD/MM/YYYY")? 'checked="checked"': ''%>> <span class="date-time-text format-i18n">15/07/2019</span> [<code>d/m/Y</code>]</label><br>
                                        <label><input type="radio" name="webDateFormat" value="DD-MM-YYYY" <%= (web && web.value && web.value.dateFormat == "d-m-Y")? 'checked="checked"': ''%>> <span class="date-time-text format-i18n">15-07-2019</span> [<code>d-m-Y</code>]</label><br>
                                        <label><input type="radio" name="webDateFormat" value="MM-DD-YYYY" <%= (web && web.value && web.value.dateFormat == "m-d-Y")? 'checked="checked"': ''%>> <span class="date-time-text format-i18n">07-15-2019</span> [<code>m-d-Y</code>]</label>
                                      </fieldset>
                                    </div>
                                  </div>
                              </div>
                              <div class="tab-pane p-20" id="homeSettings" role="tabpanel">
                                <div class="row">
                                  <div class="col-12">
                                    <div class="accordion" id="accordionHomeSettings" role="tablist">
                                    </div>
                                  </div>
                                </div>
                                <!-- Section Slider -->
                                <div class="card">
                                  <div class="card-header" role="tab" id="heading-0">
                                    <h6 class="mb-0">
                                      <a data-toggle="collapse" href="#collapse-0" aria-expanded="true" aria-controls="collapse-0" class="collapsed">
                                          <%= __('Slider') %>
                                      </a>
