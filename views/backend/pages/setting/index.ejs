                                              </div>
                                              <div class="form-group">
                                                <label><%= __('Link') %></label>
                                                <input type="text" name="link" class="form-control" value="<%= web.value.homeSettings.sliders[i].link %>" />
                                              </div>
                                            </div>
                                            <div class="col-5">
                                              <div class="form-group">
                                                <label><%= __('Headline') %></label>
                                                <input type="text" name="headline" class="form-control" value="<%= web.value.homeSettings.sliders[i].headline %>" />
