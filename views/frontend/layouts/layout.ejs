  <body>
  <!--====== PRELOADER PART START ======-->

  <div id="preloader">
      <div class="preloader">
          <span></span>
          <span></span>
      </div>
  </div>
  <!--====== PRELOADER PART ENDS ======-->

  <!--====== Header PART START ======-->
  <%- partial('../partials/header.ejs') %>
  <!--====== Header PART END ======-->

    <%if(moduleActive.action == 'frontend/home/index'){%>
      <%- partial('../partials/home-slider.ejs') %>
    <%}else if(moduleActive.action != 'frontend/event/detail' && moduleActive.action != 'frontend/gallery/detail') {%>
      <%- partial('../partials/sub-banner.ejs') %>
    <%}%>
    <%- body %>
    <%- partial('../partials/footer.ejs') %> 

    <script>delete window.self;</script>
    <!-- build:js -->
    <!--====== jquery js ======-->
    <script src="/js/frontend/vendor/modernizr-3.6.0.min.js"></script>
    <script src="/js/frontend/vendor/jquery-1.12.4.min.js"></script>

    <!--====== Bootstrap js ======-->
    <script src="/js/frontend/vendor/popper.min.js"></script>
    <script src="/js/frontend/vendor/bootstrap.min.js"></script>

     <!--====== validator-js ======-->
     <script src="/vendors/validate/validator.min.js"></script>
    <!--====== Slick js ======-->
    <script src="/js/frontend/vendor/slick.min.js"></script>
    <!--====== Magnific Popup js ======-->
    <script src="/js/frontend/vendor/jquery.magnific-popup.min.js"></script>
    <!--====== Counter Up js ======-->
    <script src="/js/frontend/vendor/waypoints.min.js"></script>
    <script src="/js/frontend/vendor/jquery.counterup.min.js"></script>
    <!--====== Nice Select js ======-->
    <script src="/js/frontend/vendor/jquery.nice-select.min.js"></script>
