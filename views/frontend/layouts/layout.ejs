    <link rel="stylesheet" href="/vendors/select2/dist/css/select2.min.css">
    <!--====== Slick css ======-->
    <link rel="stylesheet" href="/styles/frontend/slick.css">
    <!--====== Default css ======-->
    <link rel="stylesheet" href="/styles/frontend/default.css">
    <!--====== Style css ======-->
    <link rel="stylesheet" href="/styles/frontend/style.css">
    <!--====== Responsive css ======-->
    <link rel="stylesheet" href="/styles/frontend/responsive.css">
