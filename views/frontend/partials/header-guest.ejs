<!--====== Header PART START ======-->

<section class="header_area">
  <div class="header_top">
      <div class="container">
          <div class="header_top_wrapper d-flex justify-content-center justify-content-md-between">
              <div class="header_top_info d-none d-md-block">
                  <ul>
                      <li><img src="/images/call.png" alt="call"><a href="<%= webSettings && webSettings.value && webSettings.value.phoneTechnical ? 'tel:'+webSettings.value.phoneTechnical : 'javascript:void(0)' %>"><%= webSettings && webSettings.value && webSettings.value.phoneTechnical ? webSettings.value.phoneTechnical : '[YOUR_PHONE]' %></a></li>
                      <li><img src="/images/mail.png" alt="mail"><a href="<%= webSettings && webSettings.value && webSettings.value.mailTechnical ? 'mailto:'+webSettings.value.mailTechnical : 'javascript:void(0)' %>"><%= webSettings && webSettings.value && webSettings.value.mailTechnical ? webSettings.value.mailTechnical : '[YOUR_EMAIL]' %></a></li>
                  </ul>
              </div>
              <div class="header_top_login">
                  <ul>
                      <!-- <li><a href="#"><%= __('Register')%></a></li> -->
                      <li><a class="main-btn" href="/login"><i class="fa fa-user-o"></i> <%= __('Log In')%></a></li>
                  </ul>
              </div>
          </div>
      </div>
  </div>

  <div class="header_menu" style="background-color: #1b2945;">
      <div class="container">
          <nav class="navbar navbar-expand-lg">
              <a class="navbar-brand" href="/">
                  <img src="/images/kindie-logo.png" alt="Kindie">
              </a>
              <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="toggler-icon"></span>
                <span class="toggler-icon"></span>
                <span class="toggler-icon"></span>
              </button>

              <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
                  <ul class="navbar-nav m-auto">
                    <li> <a href="/"><%= __('Home')%></a> <span class="arrow"></span></li>
                    <li> <a href="/teacher"><%= __('Teacher')%></a> <span class="arrow"></span></li>
                    <li> <a href="/news"><%= __('News')%></a> <span class="arrow"></span></li>
                    <li> <a href="/gallery"><%= __('Gallery')%></a> <span class="arrow"></span></li>
                    <li> <a href="/event"><%= __('Event')%></a> <span class="arrow"></span></li>
                    <li> <a href="/about-us"><%= __('About Us')%></a> <span class="arrow"></span></li>
                    <li> <a href="/contact"><%= __('Contact')%></a> <span class="arrow"></span></li>
                  </ul>
              </div>
              
              <div class="navbar_meta">
                  <ul>
                      <li>
                          <a id="search" href="javascript:void(0)"><img src="/images/search.png" alt="search"></a>
                          <form action=/search" method="GET">
                              <div class="search_bar">
                                  <input type="text" name="keyword" placeholder="Search" required>
                                  <button type="submit"><i class="fa fa-search"></i></button>
                              </div>
                          </form>
                      </li> 
                  </ul>
              </div>
          </nav>
      </div>
  </div>
</section>
<!--====== Header PART ENDS ======-->